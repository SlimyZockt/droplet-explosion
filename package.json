{
  "name": "Droplet-Boom",
  "version": "1.1.0",
  "private": true,
  "main": "./public/main.js",
  "build": {
    "icon": "public/favicon.png",
    "productName": "Svelte and Electron App",
    "files": [
      "public/**/*",
      "venv/",
      "scripts/**/*",
      "main.js",
      "preload.js"
    ],
    "extraResources": [
      "scripts",
      "build/**/*"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "electron": "wait-on http://localhost:5000 && electron .",
    "py-dev": "tsc && concurrently \"yarn run dev\" \"yarn run python\"",
    "python": "wait-on http://localhost:5000 && \"venv/Scripts/activate.bat\" && python main.py --dev",
    "preelectron-pack": "yarn run build",
    "py-pack": "yarn run build && \"venv/Scripts/activate.bat\" && pyinstaller main.py"
  },
  "dependencies": {
    "@anpanman/opencv_ts": "^1.0.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@tsconfig/node16": "^1.0.2",
    "@vime/core": "^5.3.1",
    "@vime/svelte": "^5.3.1",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.0",
    "electron-fetch": "^1.7.4",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "install": "^0.13.0",
    "node-fetch": "^3.1.1",
    "node-sass": "^7.0.1",
    "pyodide": "^0.19.0",
    "python-shell": "^3.0.1",
    "svelte-material-ui": "^6.0.0-beta.13",
    "svelte-react-player": "^1.3.2",
    "svelte-video-player": "^1.2.5",
    "svelte-video-player-kit": "^1.2.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "concurrently": "^6.2.1",
    "electron": "^13.1.9",
    "electron-builder": "^22.14.5",
    "rollup": "^2.56.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.44.3",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.1",
    "sveltestrap": "^5.7.0",
    "tslib": "^2.0.0",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "create_svelte_electron_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
